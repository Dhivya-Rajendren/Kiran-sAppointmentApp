@page
@model Kiran_sAppointmentApp.Pages.NewPatientModel
@{
}


<form method="post">
  <div class="form-group">
    <label >Patient Name</label>
    <input asp-for=NewPatient.PatientName  class="form-control w-25"  placeholder="Enter Name">
  </div>

  <div class="form-group">
    <label>Contact </label>
    <input   asp-for=NewPatient.Contact   class="form-control w-25"  placeholder="Enter Contact">
  </div>


   <div class="form-group">
    <label >Patient Email</label>
    <input    asp-for=NewPatient.Email  class="form-control w-25"  placeholder="Enter Email">
  </div>


  <div class="form-group">
    <label>Patient History </label>
    <input   asp-for=NewPatient.PatientHistory  class="form-control w-25"  placeholder="Patient History">
  </div>
  <br/>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>